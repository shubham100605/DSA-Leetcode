class Solution {
public:
    string intToRoman(int num) {
       int digits[]={1000,900,500,400,100,90,50,40,10,9,5,4,1}; 
       string symbol[]={"M","CM","D","CD","C","XC","L","XL","X","IX","V","IV","I"};
       string ans={}; // because answer is in string format
       for(int i=0;i<13;i++){
        while(num>=digits[i]){
            ans=ans+symbol[i];
            num=num - digits[i];
        }
       
       }
        return ans;
    }
};
